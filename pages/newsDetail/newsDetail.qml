<view class="container">
  <view class="tui-news-title">
    {{article.title}}
  </view>
  <view class="tui-sub-info">
    <view class="tui-sub-left">
      <text class="tui-author">{{article.author}}</text>
      <text>{{article.time}}</text>
    </view>
    <view class="tui-sub-right"></view>
  </view>
  <view class="tui-news-content">
    <view class="tui-article">
      {{article.content}}
    </view>
    

  </view>

  

  
  

</view>